{
  "name": "@nyffels/transloco-keys-manager",
  "version": "1.1.0",
  "description": "Fork of transloco-keys-manager with extra functions",
  "main": "public_api.js",
  "scripts": {
    "release": "standard-version --infile ./CHANGELOG.md",
    "commit": "git-cz",
    "test": "cross-env PRODUCTION=true jest",
    "start": "npm run prettier && tsc --watch",
    "prebuild": "npm run clean:dist",
    "build": "tsc",
    "postbuild": "node ./scripts/post-build.js",
    "clean:dist": "rimraf dist",
    "prettier": "prettier --write 'src/**/*.ts' && prettier --write '__tests__/*.ts'",
    "conduct": "import-conductor"
  },
  "license": "MIT",
  "bin": {
    "transloco-keys-manager": "index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nyffels-IT/transloco-keys-manager.git"
  },
  "keywords": [
    "angular",
    "angular 2",
    "i18n",
    "extract",
    "transloco",
    "translate",
    "keys",
    "tool",
    "cli",
    "webpack"
  ],
  "author": "Nyffels BV",
  "maintainers": [
    {
      "name": "Shahar Kazaz"
    },
    {
      "name": "Netanel Basal"
    },
    {
      "name": "Nyffels BV",
      "email": "chesney@nyffels.be",
      "url": "https://nyffels.be"
    }
  ],
  "homepage": "https://github.com/Nyffels-IT/transloco-keys-manager#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "import-conductor --staged",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@angular/compiler": "17.3.1",
    "@ngneat/transloco-utils": "5.0.0",
    "@phenomnomnominal/tsquery": "6.1.3",
    "chalk": "5.3.0",
    "cheerio": "1.0.0-rc.12",
    "cli-table3": "0.6.4",
    "command-line-args": "5.2.1",
    "command-line-usage": "7.0.1",
    "cosmiconfig": "9.0.0",
    "debug": "4.3.4",
    "deep-diff": "1.0.2",
    "flat": "6.0.1",
    "fs-extra": "11.2.0",
    "gettext-parser": "8.0.0",
    "glob": "10.3.10",
    "jsonc-parser": "3.2.1",
    "ora": "8.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.24.3",
    "@babel/preset-typescript": "7.24.1",
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-angular": "19.1.0",
    "@commitlint/config-conventional": "19.1.0",
    "@types/cosmiconfig": "6.0.0",
    "@types/debug": "4.1.12",
    "@types/deep-diff": "1.0.5",
    "@types/flat": "5.0.5",
    "@types/gettext-parser": "4.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "29.5.12",
    "cross-env": "7.0.3",
    "git-cz": "4.9.0",
    "import-conductor": "2.6.1",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "lodash": "4.17.21",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "standard-version": "9.5.0",
    "terser": "5.30.5",
    "typescript": "5.4.3"
  }
}
